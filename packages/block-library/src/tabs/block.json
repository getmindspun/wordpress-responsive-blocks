{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "mindspun/tabs",
    "title": "Tabs",
    "category": "mindspun-responsive-blocks",
    "description": "Tabbed container.",
    "textdomain": "wpx",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css",
    "style": [
        "style-wpx",
        "file:./style.css"
    ],
    "attributes": {
        "blockId": {
            "type": "string"
        },
        "labels": {
            "type": "object",
            "default": {}
        },
        "stackOnMobile": {
            "type": "boolean",
            "default": true
        },
        "header": {
            "type": "object",
            "default": {},
            "selector": ".wpx--tabs"
        },
        "tab": {
            "type": "object",
            "default": {},
            "selector": ".wpx--tab:not(.wpx--tab-active)"
        },
        "activeTab": {
            "type": "object",
            "default": {},
            "selector": ".wpx--tab--active"
        },
        "content": {
            "type": "object",
            "default": {},
            "selector": ".wp-block-mindspun-tab"
        },
        "style": {
            "type": "object",
            "default": {}
        }
    },
    "viewScript": "file:./view.js",
    "providesContext": {
        "mindspun/tabs": "labels"
    },
    "version": "0.11.1"
}